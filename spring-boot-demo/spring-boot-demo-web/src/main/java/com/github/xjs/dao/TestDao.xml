<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.github.xjs.dao.TestDao">

	<select id="getById" resultType="com.github.xjs.domain.Test">
		select * from test where id = #{id}
	</select>
	
	<select id="list" resultType="com.github.xjs.domain.Test">
		select * from test
		order by id desc
	</select>
	
	
</mapper>